        {% if attribute.default %}{{ attribute.default|safe }}{% elif attribute.nullable %}None{% else %}...{% endif %},
        {%- for (constraint_name, constraint_value) in attribute.constraints %}
        {{ constraint_name }}={{ constraint_value|safe }},
