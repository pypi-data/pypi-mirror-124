ExampleFlightCase{
	simDefinitionFile	./MAPLEAF/Examples/Simulations/AdaptTimeStep.mapleaf

	ExpectedFinalValues{
		Mach	    	0.3514
		PositionX		-0.33166
		Time(s)	    	1.001
	}

	ParameterOverrides{

		SimControl{
			EndCondition	    Time
			EndConditionValue	1
		}
	}

	PlotsToGenerate{

		SamplePlot{
			columnsToPlot	PositionZ ^VelocityZ
			lineFormat			:b :k
			xLabel					Time (s)
			yLabel					Altitude (m)
			xLimits					0 1
			yLimits					0 100	
			plotTitle				Example Plot
			legendLabel			Altitude Sim, Velocity Sim
			saveLocation	./MAPLEAF/Examples/V&V/SamplePlot.pdf

